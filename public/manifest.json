{
    "name": "Ambient Learning",
    "description": "The expert-facing side of the spaced repetiiton system project",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "logo.png"
    },
    "action": {
        "default_title": "Click me",
        "default_icon": {
            "128": "logo.png"
        }
    },
    "background": {
        "service_worker": "./assets/serviceWorker.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./assets/content.js"]
        }
    ],
    "key": "",
    "oauth2": {
        "client_id":"1057792936287-9nn88vj3njumpt3lkuu8ln3i5g82gv4k.apps.googleusercontent.com",
        "scopes": ["profile", "email", "openid"]
    },
    "permissions": ["activeTab", "tabs", "storage", "sidePanel", "identity", "identity.email"],
    "side_panel": {
        "default_path": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["index.html"],
            "matches": ["https://*/*"]
        }
    ]
}